<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">



	<bean class="com.spring.spring_learn.Student" name="addrId">
		<property name="houseNo" value="Bunglow">
		</property>
		<property name="city" value="Badlapur">
		</property>
	</bean>
	<bean class="com.spring.spring_learn.Student" name="student1">
		<property name="empId" value="101">
		</property>
		<property name="name" value="Prathamesh">
		</property>
		<property name="address" ref="addrId">
		</property>
	</bean>
</beans>
